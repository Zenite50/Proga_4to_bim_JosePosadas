#include<iostream>
#include<conio.h>

using namespace std;

int main(){
	
 int N[10];
 int R[10];

cout<<"Ingrese 10 numeros: "<<endl;
for(int i=0;i<11;i++){	//Ciclo para almacenar los nÃºmkeros y multiplicarlos por 2.
cin>>N[i]; 

R[i]=N[i]*2;
} 

cout<<"Los numeros multiplicados por 2 son: "<<endl;
for(int j=0;j<11;j++){

cout<<R[j]<<endl;
 }		
getch;	
}
